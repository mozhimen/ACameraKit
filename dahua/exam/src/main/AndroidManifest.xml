<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".activity.NetSDKApplication"
        android:theme="@style/NetSDKBaseTheme">

        <!-- 适配全面屏 18::9 -->
        <meta-data
            android:name="android.max_aspect"
            android:value="2.1" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.MainActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.IPLoginActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.P2PLoginActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.WIFIConfigurationActivity"
            android:screenOrientation="landscape"
            android:launchMode="singleTop"
            android:windowSoftInputMode="stateHidden|adjustPan" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.DeviceSearchActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.InitDevAccountActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.FunctionListActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.LivePreviewActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.LivePreviewDoubleChannelActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.PlaybackActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.DownLoadRecordFileActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.MarkRecordActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.TalkActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.VideoTalkActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.AlarmListenActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.AlarmPushActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.AlarmControlActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.DeviceControlActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.FileBrowserActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.CapturePictureActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name=".zxing.activity.CaptureActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.MotionDetectConfig.MotionDetectConfigActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:theme="@style/NoActionBar" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.MotionDetectConfig.MotionDetectTimeSetActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:theme="@style/NoActionBar" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.MotionDetectConfig.MotionDetectTimeShowActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:theme="@style/NoActionBar" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.MotionDetectConfig.MotionDetectAreaActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:theme="@style/NoActionBar" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.APConfig.ApIPLoginActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.APConfig.ApSearchWLANListActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape"
            android:theme="@style/NoActionBar" />

        <activity
            android:name="com.mozhimen.camerak.dahua.exam.activity.APConfig.ApConfigActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <activity
            android:name=".activity.TestInterfaceActivity"
            android:launchMode="singleTop"
            android:screenOrientation="landscape" />

        <!-- [START fcm_service] -->
        <service
            android:name="com.mozhimen.camerak.dahua.exam.common.EMFCMMSGService"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="com.mozhimen.camerak.dahua.exam.common.EMFCMTokenRefreshService"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

    </application>
</manifest>